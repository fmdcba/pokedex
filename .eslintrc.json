{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:cypress/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "always",
        "json": "sometimes"
      }
    ],
    "import/prefer-default-export": [
      "warn",
      {
        "target": "single"
      }
    ]
  },
  "plugins": [
    "cypress",
    "jest",
    "prettier"
  ]
}
